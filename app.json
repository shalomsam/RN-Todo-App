{
    "expo": {
        "name": "Organiizr",
        "scheme": "com.shalomsam.organiizr",
        "description": "This is an organizer/checklist app build to help you better manage and get notified of your daily tasks. This app is built using react native expo, and has a tutorial which you can find at https://techunderthesun.in/making-a-simple-todo-mobile-app-react-native/",
        "slug": "simple-todo-native",
        "privacy": "public",
        "platforms": [
            "ios",
            "android",
            "web"
        ],
        "facebookScheme": "fb320848255809205",
        "version": "0.0.4",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.shalomsam.organiizr",
            "buildNumber": "0.0.4",
            "config": {
                "googleSignIn": {
                    "reservedClientId": "com.googleusercontent.apps.672467341657-gk7mg9m0b2t80ult8asun7h6ekevue92"
                }
            },
            "googleServicesFile": "./GoogleService-Info.plist"
        },
        "android": {
            "package": "com.shalomsam.organiizr",
            "googleServicesFile": "./google-services.json",
            "versionCode": 380000004
        },
        "hooks": {
            "postPublish": [
                {
                    "file": "sentry-expo/upload-sourcemaps"
                }
            ]
        }
    }
}
